<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<!-- Permission requires list -->
		<!-- biometric use -->
		<key>NSFaceIDUsageDescription</key>
		<string>Enhance your privacy with the Biometrics guardian</string>
		<!-- end -->
		<!-- Permission options for the `photos` group -->
		<key>NSPhotoLibraryUsageDescription</key>
		<string>Requires access to the photo library for features such as Memories and Albums.</string>
		<!-- Permission options for the `calendar` group -->
		<key>NSCalendarsUsageDescription</key>
		<string>Calendars access for feature such as Anniversaries</string>
		<key>NSCalendarsFullAccessUsageDescription</key>
		<string>Requires Calendar full access for feature such as Anniversaries.</string>
		<!-- Permission options for the `location` group -->
		<!-- <key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
    	<string>Always and when in use!</string> -->
		<key>NSLocationWhenInUseUsageDescription</key>
		<string>Requires location when in use for feature such as Memories.</string>
		<key>NSLocationUsageDescription</key>
    	<string>Requires location for feature such as Memories.</string>
		<!-- Permission options for the `mediaLibrary` group -->
		<key>NSAppleMusicUsageDescription</key>
		<string>Requires Music access!</string>
		<key>kTCCServiceMediaLibrary</key>
		<string>Requires media access for feature such as Memories and Albums.</string>
		<key>NSUserNotificationsUsageDescription</key>
		<string>Allows to send notifications, so you won't miss anything important!</string>
		<!-- end of the permission list -->
		<key>CFBundleDevelopmentRegion</key>
		<string>$(DEVELOPMENT_LANGUAGE)</string>
		<key>CFBundleDisplayName</key>
		<string>OurJourneys</string>
		<key>CFBundleExecutable</key>
		<string>$(EXECUTABLE_NAME)</string>
		<key>CFBundleIdentifier</key>
		<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
		<key>CFBundleInfoDictionaryVersion</key>
		<string>6.0</string>
		<key>CFBundleName</key>
		<string>ourjourneys</string>
		<key>CFBundlePackageType</key>
		<string>APPL</string>
		<key>CFBundleShortVersionString</key>
		<string>$(FLUTTER_BUILD_NAME)</string>
		<key>CFBundleSignature</key>
		<string>????</string>
		<key>CFBundleVersion</key>
		<string>$(FLUTTER_BUILD_NUMBER)</string>
		<key>LSRequiresIPhoneOS</key>
		<true/>
		<key>UILaunchStoryboardName</key>
		<string>LaunchScreen</string>
		<key>UIMainStoryboardFile</key>
		<string>Main</string>
		<key>UISupportedInterfaceOrientations</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>
		<key>UISupportedInterfaceOrientations~ipad</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationPortraitUpsideDown</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>
		<key>CADisableMinimumFrameDurationOnPhone</key>
		<true/>
		<key>UIApplicationSupportsIndirectInputEvents</key>
		<true/>
		<key>UIStatusBarHidden</key>
		<true/>
		<key>UIViewControllerBasedStatusBarAppearance</key>
		<false/>
	</dict>
</plist>
